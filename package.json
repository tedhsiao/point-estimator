{
  "dependencies": {
    "lodash": "^4.17.4",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3"
  },
  "devDependencies": {
    "lint-staged": "^3.4.0",
    "pre-commit": "^1.2.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
